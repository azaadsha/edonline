
{{ form.form_build_id }}
{{ form.form_token }}
{{ form.form_id }}


<div class="profile_edit_wrap">
{% if form.field_first_name['#access'] %}<div class="col-lg-4">{{form.field_first_name}}</div>{% endif %}
{% if form.field_last_name['#access'] %}<div class="col-lg-4">{{form.field_last_name}}</div>{% endif %}
{% if form.account.mail['#access'] %}<div class="col-lg-4">{{form.account.mail}}</div>{% endif %}
{% if form.account.current_pass %}<div class="col-lg-4">{{form.account.current_pass}}</div>{% endif %}
<div class="col-lg-4">{{form.account.pass.pass1}}</div>
<div class="col-lg-4">{{form.account.pass.pass2}}</div>
{% if form.field_grade_year['#access'] %}<div class="col-lg-4">{{form.field_grade_year}}</div> {% endif %}
{% if form.field_level['#access'] %}<div class="col-lg-4">{{form.field_level}}</div>{% endif %}
{#{{dump(form.field_level)}}#}
{% if form.field_parent_email['#access'] %}<div class="col-lg-4">{{form.field_parent_email}}</div>{% endif %}
{% if form.field_parent_phone['#access'] %}<div class="col-lg-4">{{form.field_parent_phone}}</div>{% endif %}

{% if form.field_phone['#access'] %}<div class="col-lg-4">{{form.field_phone}}</div>{% endif %}
{% if form.field_school['#access'] %}<div class="col-lg-4">{{form.field_school}}</div>{% endif %}
{% if form.field_date_of_birth['#access'] %}<div class="col-lg-4">{{form.field_date_of_birth}}</div>{% endif %}
{% if form.field_department['#access'] %}<div class="col-lg-4">{{form.field_department}}</div>{% endif %}
{% if form.field_date_of_joining['#access'] %}<div class="col-lg-4">{{form.field_date_of_joining}}</div>{% endif %}

{% if form.field_is_qualified['#access'] %}<div class="col-lg-4">{{form.field_is_qualified}}</div>{% endif %}



{% if form.field_profile_picture['#access'] %}<div class="col-lg-10">{{form.field_profile_picture}}</div>{% endif %}

<div class="col-lg-8 ed_prof_submit">
{{ form.actions.submit }}
</div>
</div>